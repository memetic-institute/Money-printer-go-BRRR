{
  "name": "money-printer-go-brrr",
  "description": "Print it, baby!",
  "homepage": "https://brrr.money",
  "repository": "github:memetic-institute/money-printer-go-brrr.git",
  "author": "IMRD <hi@memetic.institute> (https://memetic.institute)",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "deploy": "vercel --prod",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo lint-staged"
    }
  },
  "lint-staged": {
    "!(.next|.vercel|out|coverage|node_modules)/**/!(*test).(js|ts|tsx)": [
      "prettier --write",
      "next lint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^6.0.0-beta3",
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-brands-svg-icons": "^6.0.0-beta3",
    "@fortawesome/pro-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "csshake": "https://github.com/elrumordelaluz/csshake.git",
    "focus-visible": "^5.2.0",
    "next": "12.0.8",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "react-player": "^2.9.0",
    "react-slider": "^1.3.1",
    "react-ts-tradingview-widgets": "^1.0.8",
    "responsive-loader": "^2.3.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.8",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-slider": "^1.3.1",
    "@types/styled-components": "^5.1.20",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
